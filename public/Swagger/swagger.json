{
    "openapi": "3.0.0",
    "paths": {
        "/mobiles": {
            "get": {
                "summary": "Return a list of phones ressource",
                "operationId": "32acfc985d6def6ddc382e6d6df6ee19",
                "responses": {
                    "200": {
                        "description": "List of the mobile phones",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Product"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "JWT Token not found or expired"
                    },
                    "404": {
                        "description": "Page not found"
                    }
                },
                "security": [
                    {
                        "scalar": "bearer"
                    }
                ]
            }
        },
        "/mobiles/{id}": {
            "get": {
                "summary": "Return phone details",
                "operationId": "358dda1353c9977c9bb71fa5de88224a",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id of the mobile phone to get",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Details for one mobile",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Product"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "JWT Token not found or expired"
                    },
                    "404": {
                        "description": "Page not found"
                    }
                },
                "security": [
                    {
                        "scalar": "bearer"
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Product": {
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "price": {
                        "type": "number",
                        "format": "float"
                    },
                    "description": {
                        "type": "string"
                    },
                    "brand": {
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    }
}